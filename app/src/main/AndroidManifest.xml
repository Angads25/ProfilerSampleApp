<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.angads25.profilersampleapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".ProfilerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProfilerSampleApp">

        <activity android:name="com.github.angads25.profilersampleapp.mainscreen.view.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.github.angads25.profilersampleapp.mainscreen.view.ImageGridActivity"
            android:screenOrientation="portrait"/>
    </application>
</manifest>